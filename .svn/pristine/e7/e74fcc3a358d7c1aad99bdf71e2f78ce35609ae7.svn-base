.window {
    /* border: 1px solid black; */
    color: #000000;
    height: 120px;
    width: 120px;
    margin: 20px;
    display: flex;
    align-items: flex-end;
    overflow: hidden;
    flex-direction: column;
}
.canvas {
    /* cursor: url('./graphic/hammer.png'), auto; */
    background-image: url('./graphic/all.jpg');
    /* border: 1px solid black; */
    height: 576px;
    width: 1024px;
    float: left;
    position: relative;
    display: flex;
    flex-flow: wrap;
    justify-content: space-around;
}
/* .canvas:active {
    cursor: url('./graphic/hammerOnClick.png'), auto;
} */
.prof {
    background: url('./graphic/professor.png');
    height: 120px;
    width: 80px;
    margin: 0 auto;
    position: relative;
    bottom: -120px;
    flex: 0 1 auto;
}
.trustee {
    background: url('balloon.jpg');
    position: relative;
    border: 1px green;
    height: 120px;
    width: 80px;
    margin: 0 auto;
    bottom: -120px;
}
.windowArea {
    /* border: 1px solid black; */
    position: relative;
    display: flex;
    flex-flow: row wrap;
    align-items: center;
    justify-content: space-around;
    width: 390px;
    height: 180px;
}
.sideMenu {
    border: 1px solid blue;
    float: left;
    width: 100px;
    height: 576px;
    justify-items: center;
}

.clock
{
  position:absolute;
  top: 15%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 120px;
  height: 120px;
  border: 9px solid #ff5f5f;
  border-radius: 50%;
  background: #fff;
  box-shadow: -2px 2px 0 #e23232, insert 0 0 20px rgba(0,0,0,0.5);
}

.hand{
  width:50%;
  height:5px;
  background: black;
  position: absolute;
  top: 50%;
  transform-origin: 100%;
  transform: rotate(90deg);
  transition: all 0.5s;
}
