<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Whack a Professor!</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--Load css file-->
    <link rel="stylesheet" href="WhackAMoleCss.css">
</head>
<style>
</style>

<body>
    <!-- Importing jQuery library for animation purposes -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://code.jquery.com/ui/1.12.0/jquery-ui.js"></script>
    <!--Draw a large canvas and populate with nested "windows" and "prof" blocks-->
    <!--Main div element: the canvas-->
    <div id="main">
        <div class="canvas">
            <!--First div element: the 'window1'-->
            <div id="1" class="window1 window1">
                <!--The next nested element: the prof block and accompanying image-->
                <div class="prof"></div>
            </div>

            <!--Second div element: the 'window2'-->
            <div id="2" class="window2 window2">
                  <div class="prof"></div>
            </div>

            <!--Third div element: the 'window3'-->
            <div id="3" class="window3 window3">
                <div class="prof"></div>
            </div>

            <!--Forth div element: the 'window4'-->
            <div id="4" class="window4 window4">
                <div class="prof"></div>
            </div>

            <!--Clock div element: the 'window'-->
            <div class="clock">
                <!--The next nested element: The clock hand-->
                <div class="hand"></div>
            </div>

            <!--Fifth div element: the 'window5'-->
            <div id="5" class="window5 window5">
                <div class="prof"></div>
            </div>

            <!--Sixth div element: the 'window6'-->
            <div id="6" class="window6 window 6">
                <div class="prof"></div>
            </div>

            <!--Seventh div element: the 'window7'-->
            <div id="7" class="window7 window7">
                <div class="prof"></div>
            </div>

            <!--Eighth div element: the 'window8'-->
            <div id="8" class="window8 window8">
                <div class="prof"></div>
            </div>

            <!--Ninth div element: the 'window9'-->
            <div id="9" class="window9 window9">
                <div class="prof"></div>
            </div>

            <!--tenth div element: the 'window10'-->
            <div id="10" class="window10 window10">
                <div class="prof"></div>
            </div>

            <!--eleventh div element: the 'window11'-->
            <div id="11" class="window11 window11">
                <div class="prof"></div>
            </div>

            <!--tweleth div element: the 'window12'-->
            <div id="12" class="window12 window12">
                <div class="prof"></div>
            </div>
        </div>
      </div>

        <div class="sideMenu">
            <button id="start" onClick="startGame()">Start!</button>
            <!--Reset button - subject to change-->
            <button id="restart" onClick="restart()">Restart!</button>
            <!--Score counter - subject to change-->
            <div>Score:
                <span id="score">0</span>
            </div>
        </div>

    <!--Load javascript file -->
    <script src="WhackAMoleJS.js"></script>
</body>

</html>
