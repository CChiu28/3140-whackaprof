.clock
{
  position:absolute;
  top: 95px;
  left: 520px;
  transform: translate(-50%, -50%);
  width: 120px;
  height: 120px;
  border: 9px solid #ff5f5f;
  border-radius: 50%;
  background: #fff;
  box-shadow: -2px 2px 0 #e23232, insert 0 0 20px rgba(0,0,0,0.5);
}

.hand{
  width:50%;
  height:5px;
  background: black;
  position: absolute;
  top: 50%;
  transform-origin: 100%;
  transform: rotate(90deg);
  transition: all 0.00000000001s;
}
