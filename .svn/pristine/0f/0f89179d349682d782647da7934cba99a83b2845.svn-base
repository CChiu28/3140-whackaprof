<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Testing</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
</head>
<!-- Following CSS elements to draw simple canvas and blocks -->
<style>
    .hole {
        border: 1px solid black;
        color: #000000;
        height: 120px;
        width: 120px;
        margin: 20px;
        display: flex;
        align-items: flex-end;
    }
    .canvas {
        border: 1px solid black;
        height: 480px;
        width: 740px;
        position: relative;
        display: flex;
        flex-direction: row;
        align-items: center;
        justify-content: center;
    }
    .prof {
        height: 30px;
        width: 30px;
        color: red;
        background-color: red;
        margin: 0 auto;
        position: relative;
        bottom: -30px;
    }
</style>
<body>
    <!-- Importing jQuery library for animation purposes -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <button onClick="startGame()">Start!</button> <!--Start game on click-->
    <!--Draw a large canvas and populate with nested "holes" and "prof" blocks-->
    <div class="canvas"> <!--Main div element: the canvas-->
        <div id="1" class="hole hole1"> <!--First nested div element: the 'window'-->
            <div class="prof"></div> <!--The next nested element: the prof block-->
        </div>
        <div id="2" class="hole hole2">
                <div class="prof"></div>
        </div>
        <div id="3" class="hole hole3">
                <div class="prof"></div>
        </div>
        <div id="4" class="hole hole4">
                <div class="prof"></div>
        </div>
    </div>
    <script>
            // const windows = document.querySelectorAll(".hole");
            var check = false;
            // Function to start game
            function startGame() {
                //Gathers all div elements of class 'prof' and puts them in a NodeList
                var profs = document.getElementsByClassName("prof");
                move(profs); // Pass profs NodeList into move up function
            }
            function move(profs) {
                // jQuery method that moves blocks upward by incrementing
                // the 'bottom' property at speed 50
                $(profs).animate({bottom: '30px'},100);
                $(profs).animate({bottom: '-30px'},100);
            }
    </script>
</body>
</html>