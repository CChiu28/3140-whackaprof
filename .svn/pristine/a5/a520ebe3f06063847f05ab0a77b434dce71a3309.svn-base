.hole {
    border: 1px solid black;
    color: #000000;
    height: 120px;
    width: 120px;
    margin: 20px;
    display: flex;
    align-items: flex-end;
    overflow: hidden;
}
.canvas {
    border: 1px solid black;
    height: 480px;
    width: 740px;
    position: relative;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
}
.prof {
    height: 80px;
    width: 80px;
    color: red;
    background-color: red;
    margin: 0 auto;
    position: relative;
    bottom: -80px;
}