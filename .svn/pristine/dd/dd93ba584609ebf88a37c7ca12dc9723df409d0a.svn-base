.window {
    border: 1px solid black;
    color: #000000;
    height: 120px;
    width: 120px;
    margin: 20px;
    display: flex;
    align-items: flex-end;
    overflow: hidden;
}
.canvas {
    cursor: url('./graphic/hammer.png'), auto;
    background-image: url('./graphic/all.jpg');
    border: 1px solid black;
    height: 576px;
    width: 1024px;
    position: relative;
    display: flex;
    flex-flow: wrap;
    justify-content: space-around;
}
.canvas:active {
    cursor: url('./graphic/hammerOnClick.png'), auto;
}
.prof {
    background: url('./graphic/professor.png');
    height: 80px;
    width: 80px;
    margin: 0 auto;
    position: relative;
    bottom: -80px;
}
.windowArea {
    border: 1px solid black;
    position: relative;
    display: flex;
    flex-flow: row wrap;
    align-items: center;
    justify-content: space-around;
    width: 390px;
    height: 180px;
}